{
  "expo": {
    "name": "paid-mobile",
    "slug": "paid-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "paidmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.paidmobile",
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to select people who owe you money."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.paidmobile",
      "permissions": ["READ_CONTACTS"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      [
        "expo-contacts",
        {
          "contactsPermission": "This app needs access to your contacts to select people who owe you money."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
